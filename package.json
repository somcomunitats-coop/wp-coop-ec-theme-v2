{
  "name": "wp-coop-ce-theme-v-2",
  "version": "2.0.48",
  "description": "Child theme boilerplate to be used with WP Coop Theme",
  "author": "CÃ²dec Cooperativa",
  "repository": {
    "type": "git",
    "url": "https://git.coopdevs.org/coopdevs/comunitats-energetiques/wordpress/wp-coop-ce-theme-v-2/"
  },
  "theme_uri": "https://git.coopdevs.org/coopdevs/comunitats-energetiques/wordpress/wp-coop-ce-theme-v-2/",
  "author_uri": "https://www.codeccoop.org",
  "text_domain": "wp-coop-theme-child",
  "license": "GPL-3.0",
  "scripts": {
    "start": "npm run dev",
    "dev:css": "npx postcss ./src/css/index.css -o ./assets/css/theme.bundle.css --watch",
    "dev:js": "npx esbuild ./src/js/index.js --bundle  --outfile=./assets/js/theme.bundle.js --watch",
    "build:css": "npx postcss ./src/css/index.css -o ./assets/css/theme.bundle.css --no-map",
    "build:js": "npx esbuild ./src/js/index.js --bundle --minify --sourcemap --outfile=./assets/js/theme.bundle.js",
    "dev": "NODE_ENV=development concurrently \"npm run dev:css\" \"npm run dev:js\"",
    "build": "NODE_ENV=production concurrently \"npm run build:css\" \"npm run build:js\"",
    "postbuild": "sh ./.postbuild",
    "prepare": "husky"
  },
  "devDependencies": {
    "@jeffreyvr/tailwindcss-tailpress": "^2.0.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.1",
    "esbuild": "^0.20.0",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.3",
    "postcss-nested-ancestors": "^2.0.0",
    "postcss-url": "^10.1.3",
    "resolve-url-loader": "^3.1.2",
    "tailwindcss": "^3.3.5"
  }
}

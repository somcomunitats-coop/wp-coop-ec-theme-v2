  "styles": {
    "blocks": {
      "core/button": {
        "border": {
          "radius": "5px"
        },
        "color": {
          "background": "var(--wp--preset--color--brand)",
          "text": "var(--wp--preset--color--base)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--custom--spacing--sxxs)",
            "right": "var(--wp--custom--spacing--sm)",
            "bottom": "var(--wp--custom--spacing--sxxs)",
            "left": "var(--wp--custom--spacing--sm)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)",
          "fontWeight": "var(--wp--custom--font-weight--medium)"
        }
      },
      "core/calendar": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)"
        }
      },
      "core/code": {
        "border": {
          "radius": "0"
        },
        "color": {
          "background": "var(--wp--preset--color--main)",
          "text": "var(--wp--preset--color--typography-light)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--custom--spacing--sm)",
            "right": "var(--wp--custom--spacing--sl)",
            "bottom": "var(--wp--custom--spacing--sm)",
            "left": "var(--wp--custom--spacing--sl)"
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--monospace)",
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "core/columns": {
        "spacing": {
          "margin": {
            "bottom": "0px"
          }
        }
      },
      "core/cover": {
        "spacing": {
          "padding": {
            "top": 0,
            "right": 0,
            "bottom": 0,
            "left": 0
          }
        }
      },
      "core/heading": {
        "spacing": {
          "margin": {
            "top": "0px",
            "bottom": "var(--wp--custom--spacing--sxs)"
          }
        }
      },
      "core/image": {
        "spacing": {
          "margin": {
            "bottom": "var(--wp--custom--spacing--sl)"
          }
        }
      },
      "core/navigation": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "lineHeight": "var(--wp--custom--line-height--one)",
          "fontWeight": "var(--wp--custom--font-weight--medium)",
          "textTransform": "uppercase"
        },
        "color": {
          "text": "var(--wp--preset--color--typography)"
        }
      },
      "core/paragraph": {
        "spacing": {
          "margin": {
            "top": "0px",
            "bottom": "var(--wp--custom--spacing--sm)"
          }
        },
        "color": {
          "text": "var(--wp--preset--color--typography-light)"
        }
      },
      "core/post-title": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large-sxs)"
        }
      },
      "core/preformatted": {
        "border": {
          "color": "var(--wp--preset--color--main)",
          "radius": "0"
        },
        "color": {
          "background": "var(--wp--preset--color--main)",
          "text": "var(--wp--preset--color--typography-light)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--custom--spacing--sm)",
            "right": "var(--wp--custom--spacing--sl)",
            "bottom": "var(--wp--custom--spacing--sm)",
            "left": "var(--wp--custom--spacing--sl)"
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--monospace)",
          "fontSize": "var(--wp--preset--font-size--x-small)"
        }
      },
      "core/pullquote": {
        "border": {
          "color": "var(--wp--preset--color--main)",
          "style": "solid",
          "width": "1px"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--custom--spacing--sl)",
            "right": "var(--wp--custom--spacing--sl)",
            "bottom": "var(--wp--custom--spacing--sl)",
            "left": "var(--wp--custom--spacing--sl)"
          }
        }
      },
      "core/quote": {
        "border": {
          "color": "var(--wp--preset--color--main)",
          "style": "solid !important",
          "width": "1px !important"
        },
        "spacing": {
          "margin": {
            "bottom": "35px"
          },
          "padding": {
            "top": "var(--wp--custom--spacing--sl)",
            "right": "var(--wp--custom--spacing--sxl) !important",
            "bottom": "var(--wp--custom--spacing--sl)",
            "left": "var(--wp--custom--spacing--sxl) !important"
          }
        },
        "typography": {
          "lineHeight": "var(--wp--custom--line-height--medium)"
        }
      },
      "core/spacer": {
        "spacing": {
          "margin": {
            "top": "0 !important"
          }
        }
      },
      "core/separator": {
        "color": {
          "text": "var(--wp--preset--color--main)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)"
        }
      },
      "core/site-tagline": {
        "spacing": {
          "margin": {
            "bottom": "var(--wp--custom--spacing--sxs)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "var(--wp--custom--font-weight--bold)",
          "lineHeight": "var(--wp--custom--line-height--one)",
          "textTransform": "uppercase"
        },
        "spacing": {
          "padding": {
            "left": "0"
          }
        }
      },
      "core/table": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)"
        }
      },
      "core/template-part": {
        "spacing": {
          "margin": {
            "top": "0px"
          }
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--base)",
      "text": "var(--wp--preset--color--secondary)"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)",
          "fontSize": "var(--wp--preset--font-size--x-large-sm)",
          "lineHeight": "var(--wp--custom--line-height--one)",
          "fontWeight": "var(--wp--custom--font-weight--light)"
        },
        "color": {
          "text": "var(--wp--preset--color--typography)"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)",
          "fontSize": "var(--wp--preset--font-size--x-large-sxs)",
          "lineHeight": "var(--wp--custom--line-height--one)",
          "fontWeight": "var(--wp--custom--font-weight--bold)"
        },
        "color": {
          "text": "var(--wp--preset--color--typography)"
        }
      },
      "h3": {

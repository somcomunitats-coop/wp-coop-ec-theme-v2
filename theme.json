{
  "version": 2,
  "templateParts": [],
  "customTemplates": [],
  "settings": {
    "spacing": {
      "padding": true
    },
    "appearanceTools": true,
    "color": {
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "defaultGradients": false,
      "defaultPalette": false,
      "duotone": [
        {
          "name": "Black and Primary",
          "slug": "black-and-primary",
          "colors": [ "#000", "#990099" ]
        },
        {
          "name": "Black and Secondary",
          "slug": "black-and-secondary",
          "colors": [ "#000", "#0099ff" ]
        },
        {
          "name": "Black and Tertiary",
          "slug": "black-and-tertiary",
          "colors": [ "#000", "#00cccc" ]
        }
      ],
      "gradients": [
        {
          "name": "Black to Primary",
          "slug": "black-primary",
          "gradient": "linear-gradient(135deg,rgba(0,0,0,1) 50%,var(--wp--preset--color--primary) 100%)"
        },
        {
          "name": "Black to Secondary",
          "slug": "black-secondary",
          "gradient": "linear-gradient(135deg,rgba(0,0,0,1) 50%,var(--wp--preset--color--secondary) 100%)"
        },
        {
          "name": "Black to Tertiary",
          "slug": "black-tertiary",
          "gradient": "linear-gradient(135deg,rgba(0,0,0,1) 50%,var(--wp--preset--color--tertiary) 100%)"
        }
      ],
      "palette": [
        {
          "name": "Base",
          "slug": "base",
          "color": "#fff"
        },
        {
          "name": "Typography",
          "slug": "typography",
          "color": "#464647"
        },
        {
          "name": "Brand",
          "slug": "brand",
          "color": "#E6EF00"
        },
        {
          "name": "Main",
          "slug": "main",
          "color": "#E6EFDF"
        },
        {
          "name": "MainContrast",
          "slug": "main-contrast",
          "color": "#464647"
        },
        {
          "name": "Primary",
          "slug": "primary",
          "color": "#FFFFFF"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "#FBFBFB"
        },
        {
          "name": "Tertiary",
          "slug": "tertiary",
          "color": "#000000"
        }
      ]
    },
    "custom": {
      "fontWeight": {
        "thin": 100,
        "extra-light": 200,
        "light": 300,
        "regular": 400,
        "medium": 500,
        "semi-bold": 600,
        "bold": 700,
        "extra-bold": 800,
        "black": 900
      },
      "lineHeight": {
        "one": 1,
        "heading": 1.1,
        "medium": 1.5,
        "body": 1.75
      },
      "spacing": {
        "sminimal": "2px",
        "sxxs": "15px",
        "sxs": "20px",
        "sm": "25px",
        "sl": "30px",
        "sxl": "40px",
        "sxxl": "70px",
        "sspacer": "100px"
      }
    },
    "layout": {
      "contentSize": "1700px",
      "wideSize": "100%"
    },
    "spacing": {
      "units": [ "px", "em", "rem", "vh", "vw", "%" ]
    },
    "typography": {
      "customFontSize": true,
      "dropCap": false,
      "fontFamilies": [
        {
          "fontFamily": "\"Outfit\", sans-serif",
          "name": "Outfit",
          "slug": "primary"
        },
        {
          "fontFamily": "monospace",
          "name": "Monospace",
          "slug": "monospace"
        }
      ],
      "fontSizes": [
        {
          "name": "xSmall",
          "slug": "x-small",
          "size": "16px"
        },
        {
          "name": "Small",
          "slug": "small",
          "size": "18px"
        },
        {
          "name": "Medium",
          "slug": "medium",
          "size": "20px"
        },
        {
          "name": "Large",
          "slug": "large",
          "size": "23px"
        },
        {
          "name": "xLarge",
          "slug": "x-large",
          "size": "28px"
        },
        {
          "name": "xLarge-sxs",
          "slug": "x-large-sxs",
          "size": "clamp(28px, 3vw, 55px)"
        },
        {
          "name": "xLarge-sm",
          "slug": "x-large-sm",
          "size": "clamp(30px, 4vw, 65px)"
        },
        {
          "name": "xLarge-sl",
          "slug": "x-large-sl",
          "size": "clamp(36px, 5vw, 70px)"
        },
        {
          "name": "xLarge-sxl",
          "slug": "x-large-sxl",
          "size": "clamp(48px, 6vw, 72px)"
        }
      ]
    }
  },
  "styles": {
    "blocks": {
      "core/button": {
        "border": {
          "color": "var(--wp--preset--color--main)",
          "radius": "0",
          "style": "solid",
          "width": "1px"
        },
        "color": {
          "background": "var(--wp--preset--color--main)",
          "text": "var(--wp--preset--color--main-contrast)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--custom--spacing--sxxs)",
            "right": "var(--wp--custom--spacing--sm)",
            "bottom": "var(--wp--custom--spacing--sxxs)",
            "left": "var(--wp--custom--spacing--sm)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontWeight": "var(--wp--custom--font-weight--medium)"
        }
      },
      "core/calendar": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)"
        }
      },
      "core/code": {
        "border": {
          "radius": "0"
        },
        "color": {
          "background": "var(--wp--preset--color--main)",
          "text": "var(--wp--preset--color--main-contrast)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--custom--spacing--sm)",
            "right": "var(--wp--custom--spacing--sl)",
            "bottom": "var(--wp--custom--spacing--sm)",
            "left": "var(--wp--custom--spacing--sl)"
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--monospace)",
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "core/columns": {
        "spacing": {
          "margin": {
            "bottom": "0px"
          }
        }
      },
      "core/cover": {
        "spacing": {
          "padding": {
            "top": "var(--wp--custom--spacing--sl)",
            "right": "var(--wp--custom--spacing--sl)",
            "bottom": "var(--wp--custom--spacing--sl)",
            "left": "var(--wp--custom--spacing--sl)"
          }
        }
      },
      "core/group": {
        "spacing": {
          "padding": "0px"
        }
      },
      "core/heading": {
        "spacing": {
          "margin": {
            "top": "0px",
            "bottom": "var(--wp--custom--spacing--sxxs)"
          }
        },
        "typography": {
          "fontWeight": "var(--wp--custom--font-weight--medium)",
          "lineHeight": "var(--wp--custom--line-height--heading)"
        }
      },
      "core/image": {
        "spacing": {
          "margin": {
            "bottom": "var(--wp--custom--spacing--sl)"
          }
        }
      },
      "core/navigation": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "core/paragraph": {
        "spacing": {
          "margin": {
            "top": "0px",
            "bottom": "var(--wp--custom--spacing--sl)"
          }
        }
      },
      "core/post-title": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large-sxs)"
        }
      },
      "core/preformatted": {
        "border": {
          "color": "var(--wp--preset--color--main)",
          "radius": "0"
        },
        "color": {
          "background": "var(--wp--preset--color--main)",
          "text": "var(--wp--preset--color--main-contrast)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--custom--spacing--sm)",
            "right": "var(--wp--custom--spacing--sl)",
            "bottom": "var(--wp--custom--spacing--sm)",
            "left": "var(--wp--custom--spacing--sl)"
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--monospace)",
          "fontSize": "var(--wp--preset--font-size--x-small)"
        }
      },
      "core/pullquote": {
        "border": {
          "color": "var(--wp--preset--color--main)",
          "style": "solid",
          "width": "1px"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--custom--spacing--sl)",
            "right": "var(--wp--custom--spacing--sl)",
            "bottom": "var(--wp--custom--spacing--sl)",
            "left": "var(--wp--custom--spacing--sl)"
          }
        }
      },
      "core/quote": {
        "border": {
          "color": "var(--wp--preset--color--main)",
          "style": "solid !important",
          "width": "1px !important"
        },
        "spacing": {
          "margin": {
            "bottom": "35px"
          },
          "padding": {
            "top": "var(--wp--custom--spacing--sl)",
            "right": "var(--wp--custom--spacing--sxl) !important",
            "bottom": "var(--wp--custom--spacing--sl)",
            "left": "var(--wp--custom--spacing--sxl) !important"
          }
        },
        "typography": {
          "lineHeight": "var(--wp--custom--line-height--medium)"
        }
      },
      "core/spacer": {
        "spacing": {
          "margin": {
            "top": "0 !important"
          }
        }
      },
      "core/separator": {
        "color": {
          "text": "var(--wp--preset--color--main)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)"
        }
      },
      "core/site-tagline": {
        "spacing": {
          "margin": {
            "bottom": "var(--wp--custom--spacing--sxs)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "var(--wp--custom--font-weight--medium)",
          "lineHeight": "var(--wp--custom--line-height--one)"
        }
      },
      "core/table": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)"
        }
      },
      "core/template-part": {
        "spacing": {
          "margin": {
            "top": "0px"
          }
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--base)",
      "text": "var(--wp--preset--color--typography)"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large-sm)",
          "lineHeight": "var(--wp--custom--line-height--one)"

        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large-sxs)",
          "lineHeight": "var(--wp--custom--line-height--one)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--brand)"
        }
      }
    },
    "spacing": {
      "blockGap": "var(--wp--custom--spacing--sl)",
      "padding": {
        "top": "0px",
        "right": "0px",
        "bottom": "0px",
        "left": "0px"
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--primary)",
      "fontSize": "var(--wp--preset--font-size--x-small)",
      "fontWeight": "var(--wp--custom--font-weight--regular)",
      "lineHeight": "var(--wp--custom--line-height--body)"
    }
  }
}
